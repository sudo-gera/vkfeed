cd ~
if ! [ -d ~/vkfeed ]
then
    git clone --depth 1 https://github.com/sudo-gera/vkfeed
fi
if ! [ -f ~/vkfeed.py ]
then
    ln -s ~/vkfeed/vkfeed.py ~/vkfeed.py
fi
cd ~/vkfeed
python3 ~/vkfeed/setup.py
echo "0KPRgdGC0LDQvdC+0LLQu9C10L3Qvi4K0KLQtdC/0LXRgNGMINCy0YDQtdC80Y8g0L3QsNC/0LjRgdCw0YLRjCDQutC+0LzQsNC90LTRgyAbWzkybXZrZmVlZBtbMG0K" | base64 -d
